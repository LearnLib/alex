            <pages>
                <index project="alex-parent">
                    <breadcrumbs>false</breadcrumbs>
                    <toc>false</toc>
                    <sections>
                        <carousel />
                        <body />
                    </sections>
                </index>
                <dependency-updates-report>
                    <toc>false</toc>
                </dependency-updates-report>
                <plugin-updates-report>
                    <toc>false</toc>
                </plugin-updates-report>
                <property-updates-report>
