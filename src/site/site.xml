<?xml version="1.0" encoding="UTF-8"?>
<project name="${project.name}">

    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>

    <custom>
        <reflowSkin>
            <theme>bootswatch-spacelab</theme>
            <brand>
                <name>${project.name}</name>
                <href>/</href>
            </brand>
            <toc>sidebar</toc>
            <breadcrumbs>true</breadcrumbs>
            <topNav>Overview|Usage|Developer resources|modules|reports</topNav>
            <bottomNav>
                <column>Overview|parent|modules</column>
                <column>Usage|Developer resources</column>
                <column>reports</column>
            </bottomNav>
            <bottomDescription quote="false">
                <![CDATA[
                <a href="http://www.cs.tu-dortmund.de/"
                   target="_blank"
                   class="text-center span3">
                    <img src="http://www.cs.tu-dortmund.de/nps/Medienpool/Bereichsbilder/tu_fk04.png"
                         title="TU Dortmund - Fakultät für Informatik"
                         class="" />
                </a>
                <a href="http://ls5-www.cs.tu-dortmund.de/"
                   target="_blank"
                   class="span3 text-center">
                    <img src="http://ls5-www.cs.tu-dortmund.de/cms/Medienpool/Bereichsbilder/tu_ls_5_PS.png"
                         title="TU Dortmund - Fakultät für Informatik - Lehrstuhl 5 für Programmiersysteme"
                         class="" />
                </a>
                ]]>
            </bottomDescription>
        </reflowSkin>
    </custom>

    <version position="right" />
    <publishDate format="dd. MMM yyyy" position="right" />

    <poweredBy>
        <logo name="TU Dortmund - Fakultät für Informatik" href="http://www.cs.tu-dortmund.de/"
              img="http://www.cs.tu-dortmund.de/nps/Medienpool/Bereichsbilder/tu_fk04.png" />
        <logo
                name="TU Dortmund - Fakultät für Informatik - Lehrstuhl 5 für Programmiersysteme"
                href="http://ls5-www.cs.tu-dortmund.de/"
                img="http://ls5-www.cs.tu-dortmund.de/cms/Medienpool/Bereichsbilder/tu_ls_5_PS.png" />
    </poweredBy>

    <body>

        <breadcrumbs>
            <item name="Home" href="index.html" />
        </breadcrumbs>

        <menu name="Overview" inherit="top">
            <item name="Introduction" href="index.html" />
        </menu>

        <menu ref="parent" inherit="top" />
        <menu ref="modules" inherit="top" />

        <menu ref="reports" inherit="bottom" />

    </body>
</project>
